

CREATE COMPUTE MODULE testflow_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
		DECLARE inRef REFERENCE TO InputRoot.XMLNSC.DETAILS;
		DECLARE outRef REFERENCE TO OutputRoot.XMLNSC.DETAILS;
		DECLARE count INTEGER CARDINALITY(inRef.EMP[]);
		DECLARE i INTEGER 1;
		WHILE i<= count DO
			SET outRef.EMP[i].ENAME= inRef.EMP[i].ENAME;
			SET outRef.EMP[i].LOCATION= inRef.EMP[i].LOCATION;
			SET outRef.EMP[i].BATCH= inRef.EMP[i].BATCH;
			SET outRef.EMP[i].COMPANY=inRef.EMP[i].COMPANY;
			SET i=i+1;
		END WHILE;
		SET OutputRoot.XMLNSC.EMPLOYEEDETAILS.NAME = InputRoot.XMLNSC.EMPLOYEEDETAILS.NAME;
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
